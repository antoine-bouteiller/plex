{
  "scripts": {
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "build": "tsc -p tsconfig.prod.json && cp package.json build/package.json",
    "start": "node bin/server.js",
    "dev": "NODE_ENV=development tsx watch bin/server.ts",
    "test": "tsx bin/test.ts"
  },
  "type": "module",
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#types/*": "./types/*.js",
    "#services/*": "./app/services/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js",
    "#prisma": "./prisma/client/index.js",
    "#utils/*": "./utils/*.js"
  },
  "prisma": {
    "seed": "tsx prisma/seeder.ts"
  },
  "dependencies": {
    "@grammyjs/conversations": "^2.1.0",
    "@grammyjs/hydrate": "^1.4.1",
    "@prisma/adapter-better-sqlite3": "^6.8.2",
    "@prisma/client": "6.8.2",
    "@vinejs/vine": "^3.0.1",
    "better-sqlite3": "^11.10.0",
    "cron": "^4.3.0",
    "dotenv": "^16.5.0",
    "fastify": "^5.3.3",
    "grammy": "^1.36.1",
    "ky": "^1.8.1",
    "pino": "^9.7.0",
    "prisma": "^6.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@japa/assert": "^4.0.1",
    "@japa/file-system": "^2.3.2",
    "@japa/runner": "^4.2.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.15.21",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-perfectionist": "^4.13.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^59.0.1",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "tsc-watch": "^6.3.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  },
  "packageManager": "pnpm@10.11.0",
  "volta": {
    "node": "22.9.0"
  },
  "pnpm": {
    "neverBuiltDependencies": []
  }
}
