{
  "scripts": {
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "postinstall": "pnpm dlx prisma generate",
    "build": "tsc -p tsconfig.prod.json && cp package.json build/package.json",
    "start": "node bin/server.js",
    "dev": "NODE_ENV=development tsx watch bin/server.ts",
    "test": "tsx bin/test.ts"
  },
  "type": "module",
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#types/*": "./types/*.js",
    "#services/*": "./app/services/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js"
  },
  "dependencies": {
    "@prisma/client": "6.4.1",
    "axios": "^1.8.1",
    "cron": "^4.1.0",
    "dotenv": "^16.4.7",
    "eslint-plugin-perfectionist": "^4.9.0",
    "ffmpeg-ffprobe-static": "6.1.2-rc.1",
    "fluent-ffmpeg": "^2.1.3",
    "hyper-express": "^6.17.3",
    "pino": "^9.6.0",
    "prisma": "^6.4.1",
    "yaml": "^2.7.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@japa/assert": "^4.0.1",
    "@japa/file-system": "^2.3.2",
    "@japa/runner": "^4.2.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/node": "^22.13.9",
    "cross-env": "^7.0.3",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^57.0.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "tsc-watch": "^6.2.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0"
  },
  "packageManager": "pnpm@10.5.2",
  "volta": {
    "node": "22.9.0"
  },
  "pnpm": {
    "neverBuiltDependencies": []
  }
}
